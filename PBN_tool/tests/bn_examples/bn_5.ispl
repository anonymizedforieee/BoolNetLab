Agent M
	Vars:
		x0 : boolean;
		x1 : boolean;
		x2 : boolean;
		x3 : boolean;
		x4 : boolean;
		x5 : boolean;
		x6 : boolean;
	end Vars
	Actions = {none};
	Protocol:
		Other: {none};
	end Protocol
	Evolution:
		x0=true if ~x2|x3|~x6=true;
		x0=false if ~x2|x3|~x6=false;
		x1=true if (x1&x5)|(~x1&~x5)=true;
		x1=false if (x1&x5)|(~x1&~x5)=false;
		x2=true if (x2&x3&~x4)|(~x2&~x3)|(~x2&x4)|(~x3&x4)=true;
		x2=false if (x2&x3&~x4)|(~x2&~x3)|(~x2&x4)|(~x3&x4)=false;
		x3=true if x2=true;
		x3=false if x2=false;
		x4=true if (x0&~x4)|(~x0&~x1)|(x1&x4)=true;
		x4=false if (x0&~x4)|(~x0&~x1)|(x1&x4)=false;
		x5=true if x0=true;
		x5=false if x0=false;
		x6=true if (~x0&~x3)|(~x3&x5)=true;
		x6=false if (~x0&~x3)|(~x3&x5)=false;
	end Evolution
end Agent

InitStates
		M.x0=false or M.x0=true;
end InitStates
