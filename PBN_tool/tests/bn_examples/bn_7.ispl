Agent M
	Vars:
		x0 : boolean;
		x1 : boolean;
		x2 : boolean;
	end Vars
	Actions = {none};
	Protocol:
		Other: {none};
	end Protocol
	Evolution:
		x0=true if x1=true;
		x0=false if x1=false;
		x1=true if ~x1=true;
		x1=false if ~x1=false;
		x2=true if ~x1|x2=true;
		x2=false if ~x1|x2=false;
	end Evolution
end Agent

InitStates
		M.x1=false;
end InitStates
