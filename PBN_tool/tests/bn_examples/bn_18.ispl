Agent M
	Vars:
		x0 : boolean;
		x1 : boolean;
		x2 : boolean;
		x3 : boolean;
		x4 : boolean;
		x5 : boolean;
		x6 : boolean;
		x7 : boolean;
	end Vars
	Actions = {none};
	Protocol:
		Other: {none};
	end Protocol
	Evolution:
		x0=true if (x0&~x1&x2)|(~x0&~x2)|(x1&~x2)|(x1&~x3)|(~x1&x2&x3)=true;
		x0=false if (x0&~x1&x2)|(~x0&~x2)|(x1&~x2)|(x1&~x3)|(~x1&x2&x3)=false;
		x1=true if (x0&~x4&~x5&~x6)|(~x0&x4&x5)|(~x0&x4&x6)=true;
		x1=false if (x0&~x4&~x5&~x6)|(~x0&x4&x5)|(~x0&x4&x6)=false;
		x2=true if ~x4=true;
		x2=false if ~x4=false;
		x3=true if (x5&x7)|(~x5&~x7)|(x6&~x7)=true;
		x3=false if (x5&x7)|(~x5&~x7)|(x6&~x7)=false;
		x4=true if ~x1&x4&~x7=true;
		x4=false if ~x1&x4&~x7=false;
		x5=true if (x2&~x4&~x5)|(~x2&x4)=true;
		x5=false if (x2&~x4&~x5)|(~x2&x4)=false;
		x6=true if ~x6&x7=true;
		x6=false if ~x6&x7=false;
		x7=true if ~x0=true;
		x7=false if ~x0=false;
	end Evolution
end Agent

InitStates
		M.x0=false or M.x0=true;
end InitStates
