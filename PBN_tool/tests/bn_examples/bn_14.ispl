Agent M
	Vars:
		x0 : boolean;
		x1 : boolean;
		x2 : boolean;
		x3 : boolean;
		x4 : boolean;
	end Vars
	Actions = {none};
	Protocol:
		Other: {none};
	end Protocol
	Evolution:
		x0=true if ~x1&x2=true;
		x0=false if ~x1&x2=false;
		x1=true if x2|(x3&x4)|(~x3&~x4)=true;
		x1=false if x2|(x3&x4)|(~x3&~x4)=false;
		x2=true if (x0&x1)|(x0&~x2)|(~x0&~x1&x2)|(x1&~x2&~x4)|(~x1&x4)=true;
		x2=false if (x0&x1)|(x0&~x2)|(~x0&~x1&x2)|(x1&~x2&~x4)|(~x1&x4)=false;
		x3=true if (~x0&x2)|(~x1&x2)=true;
		x3=false if (~x0&x2)|(~x1&x2)=false;
		x4=true if x0&~x4=true;
		x4=false if x0&~x4=false;
	end Evolution
end Agent

InitStates
		M.x0=false or M.x0=true;
end InitStates
