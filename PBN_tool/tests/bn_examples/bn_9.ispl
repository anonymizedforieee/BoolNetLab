Agent M
	Vars:
		x0 : boolean;
		x1 : boolean;
		x2 : boolean;
		x3 : boolean;
		x4 : boolean;
	end Vars
	Actions = {none};
	Protocol:
		Other: {none};
	end Protocol
	Evolution:
		x0=true if ~x0&x4=true;
		x0=false if ~x0&x4=false;
		x1=true if (~x1&~x2)|(x2&~x3)|(~x2&x3&x4)|(~x3&~x4)=true;
		x1=false if (~x1&~x2)|(x2&~x3)|(~x2&x3&x4)|(~x3&~x4)=false;
		x2=true if x0|~x3=true;
		x2=false if x0|~x3=false;
		x3=true if ~x3=true;
		x3=false if ~x3=false;
		x4=true if x3=true;
		x4=false if x3=false;
	end Evolution
end Agent

InitStates
		M.x3=false;
end InitStates
